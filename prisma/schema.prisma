generator client {
  provider = "prisma-client-js"
}

generator pothos {
  provider = "prisma-pothos-types"
}

datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}
model UserActionsLog {
  id Int @id @default(autoincrement())
  createdAt DateTime @default(now())
  action UserAction
  email String
}

enum UserAction {
  CREATED
  DELETED
}
